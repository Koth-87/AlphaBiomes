		<selectable>true</selectable>
		<fillPercent>0.3</fillPercent>
		<size>(1,2)</size>
		<costList>
			<ChunkSlagSteel>1</ChunkSlagSteel>
		</costList>
		<rotatable>true</rotatable>
		<statBases>
			<MaxHitPoints>100</MaxHitPoints>
			<Flammability>1</Flammability>
