<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	
	<ThingDef ParentName="AB_JunkBuildingBase">
		<defName>AB_DerelictSwimmingPool</defName>
		<label>derelict swimming pool</label>
		<description>This was once a relaxing swimming pool, but it is now a fetid, stagnant pool of water.</description>
		<altitudeLayer>Building</altitudeLayer>
		<passability>Impassable</passability>
		<destroyable>false</destroyable>
		<useHitPoints>false</useHitPoints>
		<selectable>true</selectable>
		<fillPercent>0</fillPercent>
		<size>(10, 5)</size>
		<disableImpassableShotOverConfigError>true</disableImpassableShotOverConfigError>
		<rotatable>false</rotatable>
		<statBases>
			<Flammability>0</Flammability>
		</statBases>
		<graphicData>
			<texPath>Things/Building/DerelictResort/AB_DerelictPool</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<drawSize>(10,5)</drawSize>
			
		</graphicData>
		<building>
			<paintable>false</paintable>
			<claimable>false</claimable>
			<neverBuildable>true</neverBuildable>
		</building>
	</ThingDef>
