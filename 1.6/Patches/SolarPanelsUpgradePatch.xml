<?xml version="1.0" encoding="utf-8" ?>
<Patch>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Vanilla Furniture Expanded - Power</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>
				<li Class="PatchOperationAdd">
					<xpath>/Defs/ThingDef[defName="VFE_AdvancedSolarGenerator"]/comps</xpath>
					<value>
						<li Class="AlphaBiomes.CompProperties_AlcyioniteSolarConverter"/>
					</value>
				</li>
			</operations>
		</match>
	</Operation>
	<Operation Class="PatchOperationAdd">
		<xpath>/Defs/ThingDef[defName="SolarGenerator"]/comps</xpath>
		<value>
			<li Class="AlphaBiomes.CompProperties_AlcyioniteSolarConverter"/>
		</value>
	</Operation>
</Patch>