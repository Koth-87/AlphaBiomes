	<ThingDef ParentName="BuildingBase">
		<defName>AB_PropaneStorage</defName>
		<label>propane storage tank</label>
		<description>An industrial liquid storage tank meant for storing pressurized propane delivered via propane pipe network. Tanks can be designated to be siphoned for usable propane as needed. If damaged even lightly the escaping fumes will very swiftly create a gaseous fireball, the size of the resulting explosion will depend on the amount stored within.</description>
		<drawerType>MapMeshAndRealTime</drawerType>
		<graphicData>
			<texPath>Things/Building/PropaneLakes/AB_PropaneStorage</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<drawSize>(5,5)</drawSize>
			<damageData>
				<cornerTL>Damage/Corner</cornerTL>
				<cornerTR>Damage/Corner</cornerTR>
				<cornerBL>Damage/Corner</cornerBL>
				<cornerBR>Damage/Corner</cornerBR>
			</damageData>
		</graphicData>
		<rotatable>false</rotatable>
		<altitudeLayer>Building</altitudeLayer>
		<passability>Impassable</passability>
		<pathCost>50</pathCost>
		<blockWind>true</blockWind>
		<castEdgeShadows>true</castEdgeShadows>
		<fillPercent>0.40</fillPercent>
		<tickerType>Normal</tickerType>
		<statBases>
			<MaxHitPoints>150</MaxHitPoints>
			<WorkToBuild>800</WorkToBuild>
			<Mass>20</Mass>
			<Flammability>1.0</Flammability>
		</statBases>
		<size>(2,2)</size>
		<costList>
			<Steel>50</Steel>
		</costList>
		<comps>
			<li Class="PipeSystem.CompProperties_ResourceStorage">
				<pipeNet>AB_PropaneNet</pipeNet>
				<storageCapacity>1500</storageCapacity>
				<barSize>(1.3, 0.25)</barSize>
				<margin>0.1</margin>
				<centerOffset>(0, 0, 0.35)</centerOffset>
				<extractOptions>
					<texPath>UI/Commands/AB_ExtractPropane</texPath>
					<labelKey>AB_ExtractPropane</labelKey>
					<descKey>AB_ExtractPropaneDesc</descKey>
					<extractAmount>150</extractAmount>
					<thing>AB_Propane</thing>
					<ratio>1</ratio>
				</extractOptions>
			</li>
			<li Class="PipeSystem.CompProperties_ExplosiveContent">
				<explosiveMaxRadius>12</explosiveMaxRadius>
				<explosiveDamageType>Flame</explosiveDamageType>
				<destroyThingOnExplosionSize>2</destroyThingOnExplosionSize>
				<startWickHitPointsPercent>0.333</startWickHitPointsPercent>
				<preExplosionSpawnThingDef>AB_Filth_Propane</preExplosionSpawnThingDef>
				<preExplosionSpawnChance>1</preExplosionSpawnChance>
				<wickTicks>70~150</wickTicks>
			</li>
		</comps>
		<terrainAffordanceNeeded>Medium</terrainAffordanceNeeded>
		<designationCategory>Furniture</designationCategory>
		<staticSunShadowHeight>0.2</staticSunShadowHeight>
		<constructEffect>ConstructMetal</constructEffect>
		<canOverlapZones>false</canOverlapZones>
		<researchPrerequisites>
			<li>BiofuelRefining</li>
		</researchPrerequisites>
		<disableImpassableShotOverConfigError>true</disableImpassableShotOverConfigError>
	</ThingDef>
